package main

import (
)

type Config struct {
	HeartbeatPeriod uint16
	HeartbeatTimeout uint16
	HeartbeatLimit int
	Addr string
	ConnId string
}

func InitConfig() *Config {
	return &Config{
		HeartbeatPeriod : 3,
		HeartbeatTimeout : 5,
		HeartbeatLimit : 5,
		Addr : "localhost:8001",
		ConnId : "2b5b4c9b-d665-4446-aaa5-3ce01e090475",
	}
}
